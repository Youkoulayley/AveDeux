{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "anhskohbo/no-captcha": "^3.0",
        "barryvdh/laravel-debugbar": "~3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "callmenp/lara-auth-bridge": "^2.1",
        "caouecs/laravel-lang": "~3.0",
        "consoletvs/charts": "5.*",
        "dingo/api": "v2.0.0-alpha1",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "6.3.2",
        "jorenvanhocht/laravel-share": "^2.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "~5.0",
        "maddhatter/laravel-fullcalendar": "^1.3",
        "marcelgwerder/laravel-api-handler": "^0.7.0",
        "pusher/pusher-php-server": "^3.0",
        "spatie/laravel-cookie-consent": "^2.0",
        "thomaswelton/laravel-gravatar": "~1.0",
        "znck/belongs-to-through": "^2.3"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~7.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/fiches.php",
            "app/Helpers/helpers.php",
            "app/Helpers/logs.php",
            "app/Helpers/users.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models --dir='app/Models'"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
  "minimum-stability": "dev",
  "prefer-stable": true
}
