{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.0",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "barryvdh/laravel-debugbar": "~3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "youkoulayley/laravel-api-handler": "v1.0.1",
        "consoletvs/charts": "6.*",
        "dingo/api": "^2.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "6.4.1",
        "jorenvanhocht/laravel-share": "^2.0",
        "laravel-lang/lang": "~4.0",
        "laravel/framework": "^6.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "~6.0",
        "mews/captcha": "^3.0",
        "php-parallel-lint/php-console-highlighter": "^0.5.0",
        "predis/predis": "^1.1",
        "progcode/laravel-fullcalendar": "^1.3",
        "pusher/pusher-php-server": "^3.0",
        "spatie/laravel-cookie-consent": "^2.0",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/eloquent-has-many-deep": "^1.11",
        "staudenmeir/eloquent-eager-limit": "v1.4",
        "thomaswelton/laravel-gravatar": "~1.0",
        "wapmorgan/mp3info": "^0.0.5",
        "youkoulayley/laravel-phpbb-bridge": "^1.1.2",
        "youkoulayley/podcastfeed": "v0.4.2"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.18",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~9.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/artist.php",
            "app/Helpers/channel.php",
            "app/Helpers/episode.php",
            "app/Helpers/genre.php",
            "app/Helpers/fiches.php",
            "app/Helpers/helpers.php",
            "app/Helpers/images.php",
            "app/Helpers/logs.php",
            "app/Helpers/nationality.php",
            "app/Helpers/thetvdb.php",
            "app/Helpers/season.php",
            "app/Helpers/show.php",
            "app/Helpers/users.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
  "minimum-stability": "dev",
  "prefer-stable": true
}
